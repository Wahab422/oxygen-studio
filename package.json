{
  "name": "webflow-starter",
  "version": "1.0.0",
  "description": "A starter template for Webflow projects with page-based routing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node bin/build.js --dev",
    "build": "node bin/build.js",
    "lint": "eslint src --max-warnings=0 && prettier --check .",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "vercel:url": "node scripts/get-vercel-url.js"
  },
  "keywords": [
    "webflow",
    "starter",
    "esbuild"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@studio-freight/lenis": "^1.0.42"
  },
  "devDependencies": {
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "live-server": "^1.2.2",
    "prettier": "^3.1.0"
  }
}
